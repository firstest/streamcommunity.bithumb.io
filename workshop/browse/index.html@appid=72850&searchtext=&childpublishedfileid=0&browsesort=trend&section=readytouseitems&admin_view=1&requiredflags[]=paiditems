				var fnCleanup = null;

				if ( bResponsivePage )
				{
					var $TemplateContent = $J( '.responsive_page_template_content' ).hide();
					var $Footers = $J( '#footer_spacer, #footer' ).hide();
					$TemplateContent.parent().append( $Content );
					fnCleanup = function ()
					{
						$TemplateContent.show();
						$Footers.show();
					};
				}
				else
				{
					var $Header = $J( '#global_header' );
					$Header.wrap( '<center/>' );
					var $HeaderWrapper = $Header.parent();
					$Header.detach();

					var $VisiblePageElements = $J( 'body > :visible' )
					$VisiblePageElements.hide();

					$J( document.body ).append( $Content );

					fnCleanup = function ()
					{
						$HeaderWrapper.append( $Header );
						$VisiblePageElements.show();
					};
				}

				$Content.find( '#age_gate_btn_cancel' ).click( function ()
				{
					window.history.back();

										window.setTimeout( function ()
					{
						window.location = 'https://steamcommunity.com';
					}, 1000 );
				} );
				$Content.find( '#age_gate_btn_continue' ).click( function ()
				{
					$Content.remove();
					fnCleanup();

					// only for this session
					var strCookie = 'age_gate_' + appid;
					WebStorage.SetLocal( strCookie, true, true );

					// the apphub layout may be broken if it was hidden while this dialog was shown
					// (or if the user resized while this dialog was up) - just trigger a recalc
					$J( window ).trigger( 'resize' );
				} );
				$Content.find( '#ViewAllForApp' ).change( function ()
				{
					var bBypass = $J( "#ViewAllForApp" ).prop( 'checked' );
					SetAppAgeGateBypass( appid, bBypass, function ( data )
					{
						switch ( data.success )
						{
							case 1:
								break;
							default:
								ShowAlertDialog( 'Error', 'Failed to save your preferences' );
						}
					} );
				} );
			}
		} );
	});
