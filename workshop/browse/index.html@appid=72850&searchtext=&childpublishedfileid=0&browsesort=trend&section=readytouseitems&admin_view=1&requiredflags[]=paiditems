<script>
	function SelectSortMethod( url )
	{
		HideMenu( $('sortselect'), $('sortselect_options') );

		window.location = url;
	}

	function ToggleSectionVisibility( browseBar, elementID )
	{
		if ( $(elementID).visible() )
		{
			$(browseBar).className = "rightSectionBrowseBar";
			$(elementID).hide();
			SetCookie( 'workshopBrowseSectionVisibility' + elementID, "false", 10, '/workshop' );
		}
		else
		{
			$(browseBar).className = "rightSectionBrowseBar toggled";
			$(elementID).show();
			SetCookie( 'workshopBrowseSectionVisibility' + elementID, "true", 10, '/workshop' );
		}
	}
</script>
